
import java.util.Arrays;
import java.util.Scanner;
import java.awt.Graphics;
import java.awt.event.KeyEvent;
import java.awt.event.KeyListener;
import java.applet.Applet;
public class viva extends Applet implements KeyListener {
 String str;
 public void init() {
 addKeyListener(this);
 }
 public void keyPressed(KeyEvent arg0) { // TODO
 str = "key Pressed";
 showStatus("Key is Pressed");
 repaint();
 }
 public void keyReleased(KeyEvent arg0) { // TODO
 str = "key Released";
 showStatus("Key is Released");
 repaint();
 }
 public void keyTyped(KeyEvent arg0) {
EXP1
 str = "Key Typed :";
 showStatus("Key is Typed");
 repaint();
 }
 public void paint(Graphics g) {
 g.drawString(str, 50, 50);}}
